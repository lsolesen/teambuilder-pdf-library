{
    "name": "teambuilder/teambuilder-pdf-library",
    "description": "Create PDFs of of activities",
    "type": "library",
    "license": "MIT",
    "homepage": "http://github.com/teambuilder/teambuilder-pdf-library",
    "authors": [
        {
            "name": "Lars Olesen",
            "role": "lars@intraface.dk"
        }
    ],
    "require": {
        "tecnickcom/tcpdf": "6.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "squizlabs/php_codesniffer": "2.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Teambuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teambuilder\\Tests\\": "tests/"
        }
    }
}
